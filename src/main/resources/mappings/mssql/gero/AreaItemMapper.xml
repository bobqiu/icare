<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sjtu.icare.modules.gero.persistence.AreaItemDAO">

    <cache />
    
	<resultMap type="com.sjtu.icare.modules.gero.entity.AreaItemEntity" id="AreaItemEntityMap">
	    <id property="id" column="id" />
	    <result property="geroId" column="gero_id" />
	    <result property="name" column="name" />
	    <result property="period" column="period" />
	    <result property="frequency" column="frequency" />
	    <result property="notes" column="notes" />
	    <result property="delFlag" column="del_flag" />
	</resultMap>
  
    <select id="getAreaItemEntityById" parameterType="Map" resultMap="AreaItemEntityMap">

        SELECT
			*
        FROM T_AREA_ITEM
        WHERE 
            T_AREA_ITEM.id = #{Id}
  
    </select>
    
</mapper>